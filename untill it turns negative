#include<stdio.h>
int main(){
    int num, sum=0;
    printf("Enter negative number then exite the code.\n ");
    
    do{
        
        printf("Enter your number :- ");
        scanf("%d",&num);
         if(num<0){
        printf("Sum of all number = %d ",sum);
             break;   
   
        }
    
        sum+=num;
        
    }
    while(1);
       
    return 0;
    
}
