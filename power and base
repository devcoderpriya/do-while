#include<stdio.h>
int main()
{
    int base;
    printf("enter the number :");
    scanf("%d",&base);

    int power;
    printf("enter the number:");
    scanf("%d",&power);

    int result=1;
    int i=0;
    do{
      
        result=result*base;
        i=i+1;
       

    }
    while(i<power);
    printf("%d",result);
    return 0;
